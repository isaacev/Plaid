let fib := fn (N: Int, cb: (Int) => Void): Void {
  let f := fn (a: Int, b: Int): Void {
    cb(a);
    N := N - 1;
    if N > 0 {
      self(b, a + b);
    };
  };

  f(0, 1);
};

let printFib := fn(N: Int): Int {
  fib(N, fn(n: Int): Void {
    print(n);
  });
};

let sumFib := fn(N: Int): Int {
  let sum := 0;
  fib(N, fn(n: Int): Void {
    sum := sum + n;
  });
  return sum;
};

printFib(10);
print(sumFib(10));
